{
    "name": "liaraalis/oauth2-animexx",
    "description": "Animexx.de OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Pankow",
            "email": "liaraalis@animexx.de"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "animexx"
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "mockery/mockery": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "LiaraAlis\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LiaraAlis\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
